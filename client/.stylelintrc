{
  "processors": [
    "stylelint-processor-styled-components"
  ],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
  "rules": {
    "color-named": "never",
    "indentation": 2,
    "unit-allowed-list": ["px", "%", "s"]
  },
  "ignoreFiles": [
    "/node_modules/",
    "/build/",
    "/coverage/"
  ]
}